            TableroEntero[getIVE()][getJVE() + 1] = 3;
            TableroEntero[getIVE()][getJVE()] = 0;

        }

        if(x == 4){
            Tablero[getIVE()][getJVE() - 1] = o2;
            Tablero[getIVE()][getJVE()] = null;
            TableroEntero[getIVE()][getJVE() - 1] = 3;
            TableroEntero[getIVE()][getJVE()] = 0;

        }
    }


    // MOVIMIENTOS ORGANISMO VISION


    public int getIVI(){
        for(int i = 0; i < 10; i++){
            for(int j = 0; j < 10; j++){
                if(mapaEnteros()[i][j] == 4){
                    return i;
                }
            }
        }
        return 0;
    }
    public int getJVI(){
        for(int i = 0; i < 10; i++){
            for(int j = 0; j < 10; j++){
                if(mapaEnteros()[i][j] == 4){
                    return j;
                }
            }
        }
        return 0;
    }

    public int buscarComidaVi() {
        int i = getIVI();
        int j = getJVI();
        int vision = o4.getVision();
