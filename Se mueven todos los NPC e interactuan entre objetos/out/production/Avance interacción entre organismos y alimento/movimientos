    public int getIB(){
        for(int i = 0; i < 10; i++){
            for(int j = 0; j < 10; j++){
                if(mapaEnteros()[i][j] == 1){
                    return i;
                }
            }
        }
        return 0;
    }
    public int getJB(){
        for(int i = 0; i < 10; i++){
            for(int j = 0; j < 10; j++){
                if(mapaEnteros()[i][j] == 1){
                    return j;
                }
            }
        }
        return 0;
    }

    public int buscarComidaB() {
        int i = getIB();
        int j = getJB();
        int vision = o1.getVision();
        if (getIB() - vision < 0) {
            i = 0;
            for (int o = 0; o < vision; o++) {
                if(getJB() - vision < 0){
                    j = 0;
                    for (int p = 0 ; p < vision; p++) {
                        if (Tablero[o][p] instanceof AlimentoEnergia) {
                            if(o > getIB()){
                                return 1;
                            }
                            if (o < getIB()){
                                return 2;
                            }
                            else if(o == getIB()){
                                if(j > getJB()){
                                    return 3;
                                }
                                if(j < getJB()){
                                    return 4;
                                }
                            }
                        }
                    }
                }

                else{
                    for (int k = j- vision ; k < vision; k++) {
                        if (Tablero[o][k] instanceof AlimentoEnergia) {
                            if(o > getIB()){
                                return 1;
                            }
                            if (o < getIB()){
                                return 2;
                            }
                            else if(o == getIB()){
                                if(j > getJB()){
                                    return 3;
                                }
                                if(j < getJB()){
                                    return 4;
                                }
                            }
                        }
                    }
                }
            }
        }



        else {
            for (int a = i - vision; a < vision; a++) {
                if(j-vision < 0){
                    j = 0;
                    for (int h = 0; h < vision; h++) {
                        if (Tablero[a][h] instanceof  AlimentoEnergia) {
                            if (i > getIB()) {
                                return 1;
                            }
                            if (i < getIB()) {
                                return 2;
                            } else if (i == getIB()){
                                if (j > getJB()) {
                                    return 3;
                                }
                                if (j < getJB()) {
                                    return 4;
                                }
                            }
                        }
                    }
                }
                else{
                }
                for (int z = j- vision; z < vision; z++) {
                    if (Tablero[a][z] instanceof AlimentoEnergia){
                        if (i > getIB()) {
                            return 1;
                        }
                        if (i < getIB()) {
                            return 2;
                        } else if (i == getIB()) {
                            if (j > getJB()) {
                                return 3;
                            }
                            if (j < getJB()) {
                                return 4;
                            }
                        }
                    }
                }
            }
        }
        return 0;
    }

    public void moverseAcomidaB(int x){
        if(x == 1){
            Tablero[getIVI() + 1][getJVI()] = o1;
            Tablero[getIVI()][getJVI()] = null;
            TableroEntero[getIVI() + 1][getJVI()] = 4;
            TableroEntero[getIVI()][getJVI()] = 0;

        }
        if(x == 2){
            Tablero[getIVI() - 1][getJVI()] = o1;
            Tablero[getIVI()][getJVI()] = null;
            TableroEntero[getIVI() - 1][getJVI()] = 4;
            TableroEntero[getIVI()][getJVI()] = 0;

        }

        if(x == 3){
            Tablero[getIVI()][getJVI() + 1] = o1;
            Tablero[getIVI()][getJVI()] = null;
            TableroEntero[getIVI()][getJVI() + 1] = 4;
            TableroEntero[getIVI()][getJVI()] = 0;

        }

        if(x == 4){
            Tablero[getIVI()][getJVI() - 1] = o1;
            Tablero[getIVI()][getJVI()] = null;
            TableroEntero[getIVI()][getJVI() - 1] = 4;
            TableroEntero[getIVI()][getJVI()] = 0;

        }

    }




    //// MOVIMIENTOS ORGANISMO VELOCIDAD



    public int getIVE(){
        for(int i = 0; i < 50; i++){
            for(int j = 0; j < 50; j++){
                if(Tablero[i][j] == o2){
                    return i;
                }
            }
        }
        return 0;
    }
    public int getJVE(){
        for(int i = 0; i < 50; i++){
            for(int j = 0; j < 50; j++){
                if(Tablero[i][j] == o2){
                    return j;
                }
            }
        }
        return 0;
    }

    public int buscarComidaVe() {
        int i = getIVE();
        int j = getJVE();
        int vision = o2.getVision();
