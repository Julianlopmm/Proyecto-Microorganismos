    }

    public void moverseAcomidaVE(int x){
        if(x == 1){
            Tablero[getIVE() + 1][getJVE()] = o2;
            Tablero[getIVE()][getJVE()] = null;
            TableroEntero[getIVE() + 1][getJVE()] = 3;
            TableroEntero[getIVE()][getJVE()] = 0;

        }
        if(x == 2){
            Tablero[getIVE() - 1][getJVE()] = o2;
            Tablero[getIVE()][getJVE()] = null;
            TableroEntero[getIVE() - 1][getJVE()] = 3;
            TableroEntero[getIVE()][getJVE()] = 0;

        }

